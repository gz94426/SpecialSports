<script>

    $(function () {

        //获取商品颜色
        var GoodsColor =  $(".theme-signin >div").find(".Kuanshi li").html();
        //获取商品尺寸
        var GoodsChiCun = $(".theme-signin >div").find(".Chicun li").html();
       
        //选择颜色（款式）后，商品详情页左侧图片跟随变换
        $(".theme-signin >div").find(".Kuanshi li").click(function () {
            GoodsColor = $(this).html();

        })
      
        $(".theme-signin >div").find(".Chicun li").click(function () {
            GoodsChiCun = $(this).html();
        })

        //获取商品Id
        var GoodsId = $(".goods_Id").html();
        //获取商品备注---就是购物车里显示的商品具体名称
        var GoodsName = $(".goods_remark_name").html();
        //获取商品默认图路径
        var GoodsImgUrl = $(".goods_morentu").html();
        //商品价格
        var money = $("#jg").html();
        //数量 
        var cnum = $("#text_box").val();

        //商店id
        var Store = $("#Store").val();


        //加入购物车
        $("#LikBasket").click(function () {
            var GoodsMsg = GoodsId + "," + GoodsName + "," + GoodsImgUrl + "," + GoodsColor + "," + GoodsChiCun + "," + money + "," + cnum + "," + Store;

            $.post("GoodsShop", { GoodsMsg: GoodsMsg }, function (e) {
                if (e == "0") {
                    layer.open({
                        title: "系统提示",
                        content: "添加错误!",
                        yes: function () {
                            layer.closeAll();
                        }
                    })
                }
                else if (e == "1") {
                    layer.open({
                        title: "系统提示",
                        content: "已放入购物车，是否跳到购物车!",
                        btn: ["确定", "取消"],
                        yes: function () {
                            location.href = "ShopCart";
                            layer.closeAll();
                        },
                        btn2: function () {
                            layer.closeAll();
                        }
                    });
                }
                else if(e=="99") {
                    layer.open({
                        title: "系统提示",
                        content: "未登录,请先登录!",
                        yes: function () {
                            location.href = "../Login/Index?id=" + $(".goods_Id").html();
                            layer.closeAll();
                        }
                    })
                }

            });

        })

        //立即购买
        //判断是否已经登录
        $("#LikBuy").click(function () {
            $.post("Log", function (e) {

                if (e == 0)
                {
                    layer.open({
                        title: "系统提示",
                        content: "未登录,请先登录!",
                        yes: function () {
                            location.href = "../Login/Index?id=" + $(".goods_Id").html();
                            layer.closeAll();
                        }
                    })
                }
                else if (e == 1)
                {
                    $("#shopname").html($(".goods_remark_name").html());
                    $("#money").html($("#jg").html());
                    $("#hidd").val($(".goods_Id").html());
                    layer.open({
                        type:1,
                        title: "系统提示",
                        content: $("#divgm"),
                        area:["800","800"],
                        yes:function(){},
                        cancel:function(){
                            layer.closeAll();
                        }
                    })
                }
            });      
        });
        //确定购买
        $("#yes").click(function () {
            $(document).keydown(function (event) {
                //判断是否是输入的数字
                if (!isNaN(event.key)) {
                    $("#num" + nu).val(event.key).html("*");
                    nu += 1;
                }
                if (event.key == "Backspace") {
                    $("#num" + nu).val("").html("");
                    if (nu > 1)
                        nu -= 1;
                }
                if (nu > 6) {
                    nu = 6;
                    zhifu();
                }
            });


            var content = "输入支付密码";
            if ($("#J_ActualFee").attr("isgou") == "0") {
                alert("余额不足，请充值");

            }
            else {
                layer.open({
                    type: 1,
                    area: ["310px", "310px"],
                    content: $("#pass"),
                    title: "输入支付密码",
                    cancel: function () {
                        $(document).unbind("keydown");
                    }
                });
            }
        });

        var nu = 1;

        //数字键盘
        $(".cl_num").click(function () {
         
            if (!isNaN($(this).html())) {
                $("#num" + nu).val($(this).html()).html("*");
                nu += 1;
            }
            if (nu > 6) {
                zhifu();
                nu = 6;

            }
            if ($(this).html() == "×") {
                $("#num" + nu).val("").html("");
                if (nu > 1)
                    nu -= 1;
            }
            if ($(this).html() == "C") {
                nu = 1;
                $("#dis > div").val("").html("");
            }
        });


        //支付
        function zhifu() {
            //密码字符串
            var pass = "";
            //获取密码字符
            $("#dis").find(" > div").each(function () {
                pass += $(this).val();
            });
            var GoodsMsg = GoodsId + "," + GoodsName + "," + GoodsImgUrl + "," + GoodsColor + "," + GoodsChiCun + "," + money + "," + cnum + "," + Store;

            //提交后台 
            if (pass.length == 6) {
                $.post("ShopYes", { shopId: $(".goods_Id").html(), num: $("#text_box").val(), GoodsMsg: GoodsMsg, pwd: pass }, function (e) {
                    if (e == 0) {
                        layer.open({
                            title: "系统提示",
                            content: "余额不足,请及时充值!",
                            yes: function () {
                                layer.closeAll();
                            }
                        })
                    }
                    else if (e == 1) {
                        layer.open({
                            title: "系统提示",
                            content: "交易成功",
                            yes: function () {
                                location.href = "http://localhost:2484/";
                                layer.closeAll();
                            }
                        })
                    }
                });
            }
        }
        //我在想想
        $("#no").click(function () {
            layer.closeAll();
        });



       
    })

</script>

