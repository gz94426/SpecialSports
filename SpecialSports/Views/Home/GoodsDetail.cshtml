                                                <input id="text_box" name="" type="text" value="1" style="width: 30px;" />
                                                <input id="add" class="am-btn am-btn-default" name="" type="button" value="+" />
                                                <span id="Stock" class="tb-hidden">库存<span class="stock">1000</span>件</span>
                                            </dd>

