                                                    }    
                                                }
                                            </ul>
                                        </div>
                                        <div class="theme-options">
                                            <div class="cart-title">尺寸</div>
                                            <ul class="Chicun">
                                                @{  for (int i = 0; i < GElsit.Count(); i++)
                                                    {
                                                        if (i == 0)
                                                        {
                                                    <li class="sku-line selected">@GElsit[0].Goods_CodeNumber</li>
                                                            continue;
                                                        }
                                                        if (GElsit[i].Goods_Color == GElsit[i - 1].Goods_Color)
                                                        {
                                                    <li class="sku-line">@GElsit[i].Goods_CodeNumber</li>
                                                        }
                                                        else
                                                        {
                                                            break;
                                                        }
                                                    }    
